<!DOCTYPE html>
<html lang="en" >

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}

    {% block head_css %}
        <link rel="stylesheet" href="{{ asset('assets/css/dash.css') }}">


    {% endblock %}
    {% block head_js %}
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    {% endblock %}




</head>

<body>

<script type="text/javascript" src="//code.jquery.com/jquery-1.10.2.js"></script>

<div class='wrapper'>
    <div class='header'>
        <div class='title'>
            Administration Medina Shop
        </div>
        <div class='user'>
            <div class='name'>
                <a href="#" title="Profile Administrateur">Administrateur</a>
            </div>
            <div class='logout'>
                <a href="#" title="DÃ©connecter">D&#233;connecter</a>
            </div>
        </div>
    </div>
    <div class='main'>
        <div class='nav'>
            <div class='search'>
                <input placeholder='Search' type='text'>
            </div>
            <ul id='menu'>
                <li>
                    <a class='deroule_menu'>Tableau de bord</a>
                    <ul class="menu_cacher">
                        <li>
                            <a class='' href="#">Tableau de bord</a>
                        </li>
                        <li>
                            <a href="#">Voir le site</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a class='deroule_menu'>Pages</a>
                    <ul class="menu_cacher">
                        <li>
                            <a class='' href="#">Accueil</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a class="active deroule_menu">Tables</a>
                    <ul class="display_block menu_cacher">

                        <li>
                            <a class="active2 " href="{{ path('utilisateur_index') }}">>Utilisateurs</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('categorie_index') }}">>Categories</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('produit_index') }}">>Produits</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('commentaire_index') }}">Commentaires</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('store_index') }}">Stores</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('commande_index') }}">Commandes</a>
                        </li>
                        <li>
                            <a class="active2 " href="{{ path('facture_index') }}">Factures</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a class="deroule_menu" href="#">A propos</a>
                </li>
            </ul>
        </div>  <div class='content'>


            {% block body %}{% endblock %}



            </div>
            <div class="footer"><p>Merci de faire de SiteManager votre outil de cr&#233;ation.</p></div>  </div>
    </div>
</div>

<script>
    $(document).ready( function() {
        /*********** Menu ***********/
        $('.deroule_menu').click(function(){
            $(this).next().toggleClass('display_block');
        });
        /*********** /Menu ***********/

        /*********** /Theme ***********/
        $('.light').click(function() {
            $('.content').toggleClass('night');
            return false;
        });
        /*********** /Theme ***********/
    });
</script>






</body>

</html>